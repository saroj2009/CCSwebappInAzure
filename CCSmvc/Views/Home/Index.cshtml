        <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#table">Table</a></li>
            <li><a data-toggle="tab" href="#grid">Grid</a></li>
        </ul>
        <div class="tab-content">
            <div id="table" class="tab-pane fade in active">
                <div class="row">
                    @foreach (var obj in Model)
                    {
                        <div class="col-sm-4">
                            <a href=@obj.aid data-toggle="collapse">
                                <img src=@obj.Url@obj.Image class="img-circle person" alt="Random Name" width="50" height="80" />
                                <p class="text-center"><strong>@obj.Name</strong></p>
                            </a>

                            <div id=@obj.dvid class="collapse">
                                <p><b>Name:</b> @obj.Name</p>
                                <p><b>DOB:</b> @obj.DOB</p>
                                <p><b>Desc:</b> @obj.Description</p>

                            </div>
                        </div>

                    }

                </div>
            </div>
            <div id="grid" class="tab-pane fade">
                <br />
                @*<p>In progressss...</p>*@

                <div>
                    <table id="Demogrid"></table>
                    <div id="pager"></div>
                </div>
            </div>
        </div>

